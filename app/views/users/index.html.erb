<h1>Listing users</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%=user.name%></td>	
    <td><%= user.email %></td>
    <td><%= link_to 'Edit', edit_user_registration_path %></td>
  </tr>
<% end %>
</table>
